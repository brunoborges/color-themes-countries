{
  "name": "France - Tricolore Moderne",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1f28",
    "editor.foreground": "#e8ecf0",
    "activityBar.background": "#161a24",
    "activityBar.foreground": "#a8b8d4",
    "activityBarBadge.background": "#6878b8",
    "sideBar.background": "#1c2128",
    "sideBar.foreground": "#d5dae5",
    "sideBarSectionHeader.background": "#222835",
    "statusBar.background": "#2d3850",
    "statusBar.foreground": "#e8ecf0",
    "titleBar.activeBackground": "#1f2430",
    "titleBar.activeForeground": "#e8ecf0",
    "button.background": "#6878b8",
    "button.foreground": "#ffffff",
    "input.background": "#0f1218",
    "input.border": "#3a4a70",
    "focusBorder": "#8898d4",
    "list.activeSelectionBackground": "#2d3850",
    "list.hoverBackground": "#242f40"
  },
  "tokenColors": [
    {"scope": ["comment"], "settings": {"foreground": "#6a7a95", "fontStyle": "italic"}},
    {"scope": ["keyword", "storage.type", "storage.modifier"], "settings": {"foreground": "#8898d4"}},
    {"scope": ["string"], "settings": {"foreground": "#b8a8c8"}},
    {"scope": ["variable", "entity.name.variable"], "settings": {"foreground": "#a8b8d4"}},
    {"scope": ["function", "entity.name.function"], "settings": {"foreground": "#98b8e0"}},
    {"scope": ["constant.numeric"], "settings": {"foreground": "#c8b8d4"}}
  ]
}
