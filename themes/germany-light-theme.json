{
  "name": "Germany - Berlin Night (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fefefe",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#e8e8e8",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d88848",
    "sideBar.background": "#f5f5f5",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#e8e8e8",
    "statusBar.background": "#e8e8e8",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#e8e8e8",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#484848",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#888888",
    "list.activeSelectionBackground": "#f5f5f5",
    "list.hoverBackground": "#fefefe",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#2a2a2a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8e5e00"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#827242"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#8f8f8f"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#726222"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a28e5e"
      }
    }
  ]
}