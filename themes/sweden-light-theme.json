{
  "name": "Sweden - Nordic Blue (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#f8fbfd",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#e5eef8",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#6888c8",
    "sideBar.background": "#eff6fd",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#e5eef8",
    "statusBar.background": "#e5eef8",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#e5eef8",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#d8b848",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#7898d8",
    "list.activeSelectionBackground": "#eff6fd",
    "list.hoverBackground": "#f8fbfd",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#243454",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3272a2"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#829252"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#527292"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#42729a"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#92a262"
      }
    }
  ]
}