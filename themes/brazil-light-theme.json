{
  "name": "Brazil - Tropical Green (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#f8fcf8",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#e5f0e5",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d8c848",
    "sideBar.background": "#eff8ef",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#e5f0e5",
    "statusBar.background": "#e5f0e5",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#e5f0e5",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#68a868",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#88c888",
    "list.activeSelectionBackground": "#eff8ef",
    "list.hoverBackground": "#f8fcf8",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#244424",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#428242"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#829232"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#728e62"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#628252"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#929252"
      }
    }
  ]
}