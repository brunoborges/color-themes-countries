{
  "name": "China - Dragon Red (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fef8f5",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f8e8e0",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d86858",
    "sideBar.background": "#fdf0eb",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f8e8e0",
    "statusBar.background": "#f8e8e0",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f8e8e0",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#d86858",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#e88878",
    "list.activeSelectionBackground": "#fdf0eb",
    "list.hoverBackground": "#fef8f5",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#3f2a2a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a26242"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#925252"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#9a7a62"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#92725a"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a28262"
      }
    }
  ]
}