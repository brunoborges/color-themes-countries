{
  "name": "Australia - Outback Sunset (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fdfaf8",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f5ebe5",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d88848",
    "sideBar.background": "#faf3ef",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f5ebe5",
    "statusBar.background": "#f5ebe5",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f5ebe5",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#6878a8",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#e89858",
    "list.activeSelectionBackground": "#faf3ef",
    "list.hoverBackground": "#fdfaf8",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#243444",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#826222"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#927242"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#827252"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#726242"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a28252"
      }
    }
  ]
}