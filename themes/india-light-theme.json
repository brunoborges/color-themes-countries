{
  "name": "India - Saffron Dream (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fef9f5",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f8ede0",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#e89860",
    "sideBar.background": "#fdf5eb",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f8ede0",
    "statusBar.background": "#f8ede0",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f8ede0",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#68a868",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#88b888",
    "list.activeSelectionBackground": "#fdf5eb",
    "list.hoverBackground": "#fef9f5",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#243f2f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#92521a"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#528362"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#728e7a"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#7a654a"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#9a6a42"
      }
    }
  ]
}