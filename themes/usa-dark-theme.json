{
  "name": "USA - Stars & Stripes",
  "type": "dark",
  "colors": {
    "editor.background": "#1a2332",
    "editor.foreground": "#e8eef5",
    "activityBar.background": "#162030",
    "activityBar.foreground": "#b8d4f1",
    "activityBarBadge.background": "#c85a5a",
    "sideBar.background": "#1c2537",
    "sideBar.foreground": "#d5dce5",
    "sideBarSectionHeader.background": "#223047",
    "statusBar.background": "#2d4365",
    "statusBar.foreground": "#e8eef5",
    "titleBar.activeBackground": "#1f2d42",
    "titleBar.activeForeground": "#e8eef5",
    "button.background": "#c85a5a",
    "button.foreground": "#ffffff",
    "input.background": "#0f1821",
    "input.border": "#3a5174",
    "focusBorder": "#6892c7",
    "list.activeSelectionBackground": "#2d4365",
    "list.hoverBackground": "#243550"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#7a8ca0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#6892c7"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#e89595"
      }
    },
    {
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#b8d4f1"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#94b8e0"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#d4a8a8"
      }
    }
  ]
}
