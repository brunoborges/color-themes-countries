  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#3f2a2a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#922222"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#a27272"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#a28282"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#926262"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a28282"
      }
    }
  ]
}