{
  "name": "Canada - Maple Leaf (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fefafa",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f5e8e8",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d85858",
    "sideBar.background": "#fdf0f0",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f5e8e8",
    "statusBar.background": "#f5e8e8",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f5e8e8",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#d85858",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#e87878",
    "list.activeSelectionBackground": "#fdf0f0",
    "list.hoverBackground": "#fefafa",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
