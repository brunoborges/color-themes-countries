{
  "name": "Japan - Sakura Soft (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fef8fc",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f8e8f0",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#d86888",
    "sideBar.background": "#fdf0f8",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f8e8f0",
    "statusBar.background": "#f8e8f0",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f8e8f0",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#a85878",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#e8a8b8",
    "list.activeSelectionBackground": "#fdf0f8",
    "list.hoverBackground": "#fef8fc",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#3f2a2f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a26272"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#925272"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#a27282"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#926272"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a28292"
      }
    }
  ]
}