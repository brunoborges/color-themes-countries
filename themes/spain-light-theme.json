{
  "name": "Spain - Mediterranean Sun (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#fefaf5",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#f8ede0",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#e88848",
    "sideBar.background": "#fdf4eb",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#f8ede0",
    "statusBar.background": "#f8ede0",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#f8ede0",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#c85858",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#f89858",
    "list.activeSelectionBackground": "#fdf4eb",
    "list.hoverBackground": "#fefaf5",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#3f3424",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#a28222"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#927242"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#a28252"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#927232"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#a29252"
      }
    }
  ]
}