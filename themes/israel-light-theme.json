{
  "name": "Israel - Tech Aviv (Light)",
  "type": "light",
  "colors": {
    "editor.background": "#f8fafd",
    "editor.foreground": "#2c3540",
    "activityBar.background": "#e5ecf8",
    "activityBar.foreground": "#3c4550",
    "activityBarBadge.background": "#6878c8",
    "sideBar.background": "#eff5fd",
    "sideBar.foreground": "#3c4550",
    "sideBarSectionHeader.background": "#e5ecf8",
    "statusBar.background": "#e5ecf8",
    "statusBar.foreground": "#2c3540",
    "titleBar.activeBackground": "#e5ecf8",
    "titleBar.activeForeground": "#2c3540",
    "button.background": "#6878c8",
    "button.foreground": "#ffffff",
    "input.background": "#ffffff",
    "input.border": "#d0d5da",
    "focusBorder": "#7888d8",
    "list.activeSelectionBackground": "#eff5fd",
    "list.hoverBackground": "#f8fafd",
    "list.activeSelectionForeground": "#2c3540",
    "list.inactiveSelectionForeground": "#2c3540",
    "list.hoverForeground": "#2c3540"
  },
  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#243454",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3262a2"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#929292"
      }
    },
    {
      "scope": [
        "variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#5272a2"
      }
    },
    {
      "scope": [
        "function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#42729a"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#9292a2"
      }
    }
  ]
}